<%= f.fields_for "photos" do |photo| %>
  <ol class='photos'>
  <% @user_photos.each do |p| %>
    <%= photo.input :photo_id, as: :boolean, input_html: { id: p.id, value: p.id, name: 'album[photos][photo_id][]' }, label: "#{image_tag(p.images.thumbnail.url)} #{p.caption.text}".html_safe %>
  <% end %>
</ol>
<% end %>

<%= link_to 'Load', load_photos_path(max_id: @page_max_id), remote: true, id: 'load-more' %>
